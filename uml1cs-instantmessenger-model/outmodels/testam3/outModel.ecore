      <ownedElement xsi:type="uml:Class" stereotype="//@ownedElement.23" clientDependency="//@ownedElement.15/@ownedElement.10/@ownedElement.8"
          name="ConversationEdit">
        <feature xsi:type="uml:Operation" stereotype="//@ownedElement.28" name="ConversationEdit">
          <parameter name="c" type="//@ownedElement.15/@ownedElement.2/@ownedElement.8"/>
          <parameter name="return" type="//@ownedElement.15/@ownedElement.1/@ownedElement.5"
              kind="pdk_return"/>
