rule AppletInitProcedureExpression {
    from s : INMODEL!ProcedureExpression (
    	s.method().owner.isSingleton() and
    	s.method().owner.isApplet() and
		s.method().specification.name = 'init')
    to t : OUTMODEL!ProcedureExpression mapsTo s (
        language <- s.language,
        body <- s.method().owner.isSingleton().
			javaAppletInitBody() + '\n' + s.body)
}

rule ProcedureExpression {
    from s : INMODEL!ProcedureExpression (not (
    	s.method().owner.isSingleton() and
    	s.method().owner.isApplet() and
		s.method().specification.name = 'init'))
    to t : OUTMODEL!ProcedureExpression mapsTo s (
        language <- s.language,
        body <- s.body)
}

