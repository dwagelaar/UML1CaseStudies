rule AppletClass {
	from s : INMODEL!Class (s.isApplet())
	to g : OUTMODEL!Generalization (
	   	namespace <- s.namespace,
		parent <- 'Applet'.class(),
		child <- s)
}

rule NewAppletInitOperation {
	from s : INMODEL!Class (s.isSingleton() and
		s.hasNoInitOperation() and s.isApplet())
	to op : OUTMODEL!Operation (
	   	name <- 'init',
		owner <- s,
		visibility <- #vk_public,
		ownerScope <- #sk_instance,
		isAbstract <- false,
		concurrency <- #cck_sequential),
	   mbody : OUTMODEL!ProcedureExpression (
	   	language <- 'java',
		body <- s.method().owner.isSingleton().
			javaAppletInitBody()),
	   meth : OUTMODEL!Method (
	   	owner <- s,
		body <- mbody,
		specification <- op)
}

rule AppletInfo {
    from s : INMODEL!Class (
    	s.hasTaggedValue('AppletInfo'))
	to op : OUTMODEL!Operation (
        name <- 'getAppletInfo',
		owner <- s,
        visibility <- #vk_public,
		ownerScope <- #sk_instance,
		isAbstract <- false,
		concurrency <- #cck_sequential),
	   return : OUTMODEL!Parameter (
	   	name <- 'return',
		behavioralFeature <- op,
		type <- 'String'.class(),
		kind <- #pdk_return),
	   mbody : OUTMODEL!ProcedureExpression (
	   	language <- 'java',
		body <- 'return "' + s.taggedValue('AppletInfo') + '";'),
	   meth : OUTMODEL!Method (
	   	owner <- s,
		body <- mbody,
		specification <- op)
}

