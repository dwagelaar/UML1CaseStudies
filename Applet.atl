-- ======================================================================
-- non-mapsTo rules begin
-- ======================================================================

rule AppletClass {
	from s : INMODEL!Class (s.isApplet())
	to g : OUTMODEL!Generalization (
	   	namespace <- s.namespace,
		parent <- if 'java.applet.Applet'.hasQClass()
			then 'java.applet.Applet'.qClass()
			else 'java.applet.Applet'.mergeQClass() endif,
		child <- s)
}

rule NewAppletInitOperation {
	from s : INMODEL!Class (s.isApplet() and
		s.hasNoOperation('init'))
	to op : OUTMODEL!Operation (
	   	name <- 'init',
		owner <- s,
		visibility <- #vk_public,
		ownerScope <- #sk_instance,
		isAbstract <- false,
		concurrency <- #cck_sequential),
	   mbody : OUTMODEL!ProcedureExpression (
	   	language <- 'java',
		body <- s.method().owner.isSingleton().
			javaAppletInitBody()),
	   meth : OUTMODEL!Method (
	   	owner <- s,
		body <- mbody,
		specification <- op)
}

rule AppletInfo {
    from s : INMODEL!Class (
    	s.hasTaggedValue('AppletInfo'))
	to op : OUTMODEL!Operation (
        name <- 'getAppletInfo',
		owner <- s,
        visibility <- #vk_public,
		ownerScope <- #sk_instance,
		isAbstract <- false,
		concurrency <- #cck_sequential),
	   return : OUTMODEL!Parameter (
	   	name <- 'return',
		behavioralFeature <- op,
		type <- if 'java.lang.String'.hasQClass()
			then 'java.lang.String'.qClass()
			else 'java.lang.String'.mergeQClass() endif,
		kind <- #pdk_return),
	   mbody : OUTMODEL!ProcedureExpression (
	   	language <- 'java',
		body <- 'return "' + s.taggedValue('AppletInfo') + '";'),
	   meth : OUTMODEL!Method (
	   	owner <- s,
		body <- mbody,
		specification <- op)
}

rule ParameterInfo {
    from s : INMODEL!Class (
    	s.hasTaggedValue('ParameterInfo'))
    to op : OUTMODEL!Operation (
        name <- 'getParameterInfo',
		owner <- s,
        visibility <- #vk_public,
		ownerScope <- #sk_instance,
		isAbstract <- false,
		concurrency <- #cck_sequential),
	   return : OUTMODEL!Parameter (
	   	name <- 'return',
		behavioralFeature <- op,
		type <- if 'java.lang.String[][]'.hasQClass()
			then 'java.lang.String[][]'.qClass()
			else 'java.lang.String[][]'.mergeQClass() endif,
		kind <- #pdk_return),
	   mbody : OUTMODEL!ProcedureExpression (
	   	language <- 'java',
		body <- 'String[][] pinfo = ' + s.taggedValue('ParameterInfo') + ';\nreturn pinfo;'),
	   meth : OUTMODEL!Method (
	   	owner <- s,
		body <- mbody,
		specification <- op)
}

-- ======================================================================
-- non-mapsTo rules end
-- ======================================================================

-- ======================================================================
-- mapsTo rules begin
-- ======================================================================

rule AppletInitProcedureExpression {
    from s : INMODEL!ProcedureExpression (
    	s.method().owner.isSingleton() and
    	s.method().owner.isApplet() and
		s.method().specification.name = 'init')
    to t : OUTMODEL!ProcedureExpression mapsTo s (
        language <- s.language,
        body <- s.method().owner.isSingleton().
			javaAppletInitBody() + '\n' + s.body)
}

rule ProcedureExpression {
    from s : INMODEL!ProcedureExpression (not (
    	s.method().owner.isSingleton() and
    	s.method().owner.isApplet() and
		s.method().specification.name = 'init'))
    to t : OUTMODEL!ProcedureExpression mapsTo s (
        language <- s.language,
        body <- s.body)
}

