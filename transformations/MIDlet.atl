helper context String def : hasDataType() : Boolean =
	not SINGLETONM!DataType.allInstances()->select(c|c.name=self)->isEmpty();

helper context String def : dataType() : SINGLETONM!DataType =
	SINGLETONM!DataType.allInstances()->select(c|c.name=self)->first();

helper context String def : mergeDataType() : MERGEMODEL!DataType =
	MERGEMODEL!DataType.allInstances()->select(c|c.name=self)->first();

helper context String def : qClass() : SINGLETONM!Class =
	SINGLETONM!Class.allInstances()->select(c|c.qName()=self)->first();

helper context String def : mergeQClass() : MERGEMODEL!Class =
	MERGEMODEL!Class.allInstances()->select(c|c.qName()=self)->first();

helper context String def : hasQClass() : Boolean =
	not SINGLETONM!Class.allInstances()->select(c|c.qName()=self)->isEmpty();

helper context SINGLETONM!GeneralizableElement def : isApplet() : Boolean =
	not self.stereotype->select(s|s.name='Applet')->isEmpty();

helper context SINGLETONM!GeneralizableElement def : isSingleton() : Boolean =
	not self.stereotype->select(s|s.name='Singleton')->isEmpty();

helper context SINGLETONM!Classifier def : hasNoOperation(name : String) : Boolean =
	self.feature->select(f|f.name=name and
		f.oclIsKindOf(SINGLETONM!Operation))->isEmpty();

helper context SINGLETONM!ProcedureExpression def : method() : SINGLETONM!Method =
	SINGLETONM!Method.allInstances()->select(m|m.body=self)->first();

helper context SINGLETONM!ModelElement def : hasTaggedValue(name : String) : Boolean =
	if self.taggedValue->select(t|t.type.name = name)->isEmpty() then
		false
	else
		not self.taggedValue->select(t|t.type.name = name)
			->first().dataValue->isEmpty()
	endif;

helper context SINGLETONM!ModelElement def : taggedValue(name : String) : String =
	self.taggedValue->select(t|t.type.name = name)
			->first().dataValue->asSequence()->first();

