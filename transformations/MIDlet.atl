rule AppletClass {
	from s : SINGLETONM!Class (s.isApplet())
	to g : APPLETM!Generalization (
	   	namespace <- s.namespace,
		parent <- if 'javax.microedition.midlet.MIDlet'.hasQClass()
			then 'javax.microedition.midlet.MIDlet'.qClass()
			else 'javax.microedition.midlet.MIDlet'.mergeQClass() endif,
		child <- s),
	   initialized : APPLETM!Attribute (
	   	name <- 'initialized',
		owner <- s,
		type <- if 'Boolean'.hasDataType()
			then 'Boolean'.dataType()
			else 'Boolean'.mergeDataType() endif,
		visibility <- #vk_private,
		ownerScope <- #sk_instance,
		changeability <- #ck_changeable,
		initialValue <- v),
	   v : APPLETM!Expression (
	   	language <- 'java',
		body <- 'false'),
	   getParameter : APPLETM!Operation (
	   	name <- 'getParameter',
		owner <- s,
		visibility <- #vk_public,
		ownerScope <- #sk_instance,
		isAbstract <- false,
		concurrency <- #cck_sequential),
	   name : APPLETM!Parameter (
	   	name <- 'name',
		behavioralFeature <- getParameter,
		type <- if 'String'.hasDataType()
			then 'String'.dataType()
			else 'String'.mergeDataType() endif,
		kind <- #pdk_in),
	   return : APPLETM!Parameter (
	   	name <- 'return',
		behavioralFeature <- getParameter,
		type <- if 'String'.hasDataType()
			then 'String'.dataType()
			else 'String'.mergeDataType() endif,
		kind <- #pdk_return),
	   getParameterBody : APPLETM!ProcedureExpression (
	   	language <- 'java',
		body <- 'return getAppProperty(name);'),
	   getParameterMeth : APPLETM!Method (
	   	owner <- s,
		body <- getParameterBody,
		specification <- getParameter)
}

rule NewAppletStartAppOperation {
	from s : SINGLETONM!Class (s.isApplet() and
		s.hasNoOperation('startApp'))
	to op : APPLETM!Operation (
	   	name <- 'startApp',
		owner <- s,
		visibility <- #vk_public,
		ownerScope <- #sk_instance,
		isAbstract <- false,
		concurrency <- #cck_sequential),
	   mbody : APPLETM!ProcedureExpression (
	   	language <- 'java',
		body <- thisModule.javaMIDletStartAppBody()),
	   meth : APPLETM!Method (
	   	owner <- s,
		body <- mbody,
		specification <- op)
}

