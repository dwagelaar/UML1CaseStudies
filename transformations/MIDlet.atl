rule AppletInitProcedureExpression {
    from s : SINGLETONM!ProcedureExpression (
    	s.method().owner.isSingleton() and
    	s.method().owner.isApplet() and
		s.method().specification.name = 'init')
    to t : APPLETM!ProcedureExpression mapsTo s (
        language <- s.language,
        body <- s.method().owner.isSingleton().
			javaMIDletInitBody() + '\n' + s.body)
}

rule ProcedureExpression {
    from s : SINGLETONM!ProcedureExpression (not (
    	s.method().owner.isSingleton() and
    	s.method().owner.isApplet() and
		s.method().specification.name = 'init'))
    to t : APPLETM!ProcedureExpression mapsTo s (
        language <- s.language,
        body <- s.body)
}

