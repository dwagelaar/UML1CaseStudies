          </owl:Restriction>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="http://ssel.vub.ac.be/codamos/2005/03/java.owl#JavaVM"/>
            <owl:onProperty rdf:resource="http://ssel.vub.ac.be/codamos/2005/03/context.owl#providesSoftware"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="http://ssel.vub.ac.be/codamos/2005/03/context.owl#Platform"/>
  </owl:Class>
</rdf:RDF>

