<?xml version="1.0"?>
<rdf:RDF
    xmlns:context="http://ssel.vub.ac.be/codamos/2005/03/context.owl#"
    xmlns:java="http://ssel.vub.ac.be/codamos/2005/03/java.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://local/Singleton.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://local/Singleton.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://ssel.vub.ac.be/codamos/2005/03/context.owl"/>
    <owl:imports rdf:resource="http://ssel.vub.ac.be/codamos/2005/03/java.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="SingletonPlatform">
    <rdfs:subClassOf rdf:resource="http://ssel.vub.ac.be/codamos/2005/03/context.owl#Platform"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://ssel.vub.ac.be/codamos/2005/03/context.owl#providesSoftware"/>
        <owl:someValuesFrom rdf:resource="http://ssel.vub.ac.be/codamos/2005/03/java.owl#JavaVM"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
</rdf:RDF>

