	to op : OUTMODEL!Operation (
	   	name <- 'get' + s.name.stringFirstToUpper() + s.type.name.s(),
		owner <- s.owner,
		visibility <- s.visibility,
		ownerScope <- s.ownerScope,
		isAbstract <- false,
		concurrency <- #cck_sequential),
	   par : OUTMODEL!Parameter (
	   	name <- 'return',
		behavioralFeature <- op,
		type <- s.type,
		kind <- #pdk_return),
	   mbody : OUTMODEL!ProcedureExpression (
	   	language <- 'java',
		body <- s.name.javaGetter()),
	   meth : OUTMODEL!Method (
	   	owner <- s.owner,
		body <- mbody,
		specification <- op),
	   dep : OUTMODEL!Dependency (
	   	name <- 'accessor ' + 'get' + s.name.stringFirstToUpper() + s.type.name.s() + '()',
		namespace <- s.owner.namespace,
		stereotype <- if 'accessor'.hasStereotype()
